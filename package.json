{
  "name": "papu",
  "author": "Daniel Vazaƒç",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "autoVersion": "node ./scripts/autoVersion.js",
    "dev": "concurrently \"npm run dev:db\" \"npm run dev:client\"",
    "dev:client": "vite dev --mode DEV",
    "dev:db": "cd ./db && node ./localDB.js",
    "copy": "cpy package.json build && cpy 'webroot/**/*' 'build/'",
    "build": "npm run autoVersion && vite build --mode PROD && npm run copy",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "start": "node ./index.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@types/node": "^20.4.6",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.1",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.27",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.3",
    "@types/qrcode": "^1.5.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.1.0",
    "mongoose": "^7.5.3",
    "qrcode": "^1.5.3",
    "winston": "^3.10.0"
  }
}
